<h1>Edit Category</h1>

<form method="POST" action="/category/update">
    <input type="hidden" name="id" value="<%= category._id %>">
    <div>
        <label>Category Name:</label>
        <input type="text" name="name" value="<%= category.name %>">
    </div>
    <button  type="submit">Update Category</button>
    </form>
